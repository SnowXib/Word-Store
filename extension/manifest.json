{
  "manifest_version": 3,
  "name": "Word Store",
  "version": "0.1",
  "description": "Extension for storing words",
  "icons": {
    "16": "icons/word_store_16_16.png",
    "32": "icons/word_store_32_32.png",
    "48": "icons/word_store_48_48.png",
    "128": "icons/word_store_128_128.png"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "permissions": ["contextMenus", "scripting", "tabs"],
  "background": {
    "service_worker": "background.js"
  }
}
